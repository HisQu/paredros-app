{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "paredros-app",
  "version": "0.1.0",
  "identifier": "de.uni-jena.de.mephisto",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "paredros-app",
        "width": 1280,
        "height": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "resources": [],
    "externalBin": []
  }
}
